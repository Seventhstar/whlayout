<p id="notice"><%= notice %></p>

<h1>Микросхемы и т.д.</h1>

<div class="bs-example" data-example-id="simple-table">
<table class="table table-striped table-hover">

  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elements.each do |element| %>
      <tr>
        <td><%= element.name %></td>
         <%= edit_delete(element) %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Добавить элемент', new_element_path, class: "btn btn-success" %>

</div>
