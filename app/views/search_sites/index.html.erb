<div class="panel panel-default">
      <div class="panel-heading table-heading"></div>
      <table class="table table-striped table-hover" model="<%= @search_urls.class.name %>">

  <thead>
    <tr>
      <th>Name</th>
      <th>Items</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @search_sites.each do |search_site| %>
      <tr>
        <td><%= search_site.name %></td>
        <td><%= search_site.items %></td>
	<%= td_edit_delete(search_site) %>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>

<%= link_to 'Добавить', new_search_site_path , class: 'btn-success'%>
