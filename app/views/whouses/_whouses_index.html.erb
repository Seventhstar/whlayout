<% @whouses.each do |whouse| %>

<div class="panel panel-default">

<div class="panel-heading table-heading"><%= link_to whouse.name,whouse %></div>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th class="w80">Элемент</th>
      <th>Количество</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
      <% whouse.whouse_elements.each do |el| %>
      <tr>
        <td><%= el.element_name %></td>
        <td><%= el.count %></td>
        <td></td>
        
      </tr>
      <% end %>
  </tbody>
</table>
</div>
<% end %>
<br>